{
	"name": "DevBox (homedir)",
	"build": {
		"dockerfile": "../Dockerfile",
		"context": ".."
	},
  "workspaceFolder": "/home/dkl",
	"extensions": [
		"ms-vscode.azure-account",
		"ms-azuretools.vscode-docker",
		"vsls-contrib.gitdoc",
		"github.vscode-pull-request-github",
		"eamodio.gitlens",
		"cfgweb.vscode-perl",
		"richterger.perl",
		"d9705996.perl-toolbox",
		"sfodje.perltidy",
		"felixfbecker.php-debug",
		"felixfbecker.php-intellisense",
		"ms-python.python",
		"humao.rest-client",
		"lextudio.restructuredtext"
	],
	"remoteUser": "dkl",
	"containerEnv": {
		"TERM":"xterm-256color"
	},
	"mounts": [
		"source=/var/run/docker.sock,target=/var/run/docker.sock,type=bind,consistency=cached",
		"source=github-repos,target=/home/dkl/github/repos/,type=volume"
	]
}